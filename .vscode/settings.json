{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.sortMembers": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.rulers": [80],
  "cSpell.words": [
    "Dressipi",
    "Ecommerce",
    "iglu",
    "PKCE",
    "Queueable",
    "reparentable",
    "snakecase"
  ]
}
